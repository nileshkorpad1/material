  "scripts": {
    "clean": "rimraf dist build compiled",
    "watch": "webpack --watch --progress --profile",
    "build-demo": "webpack --progress --profile --bail",
    "build-demo-ci": "webpack --profile --bail",
    "build": "npm run clean && npm run build-demo && prerender",
    "build-ci": "npm run clean && npm run build-demo-ci && prerender",
    "server": "webpack-dev-server --inline --progress --port 8090",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "npm run server",
    "preinstall": "cd ../bundle && npm install && cd ../site"
  },
